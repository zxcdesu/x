generator client {
  provider        = "prisma-client-js"
  output          = "./generated"
  previewFeatures = ["nativeDistinct", "postgresqlExtensions", "relationJoins"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id String @db.Uuid @id @default(uuid())
}
