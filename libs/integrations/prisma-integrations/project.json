{
  "name": "prisma-integrations",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/integrations/prisma-integrations/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/integrations/prisma-integrations/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/integrations/prisma-integrations/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema libs/integrations/prisma-integrations/prisma/schema.prisma"
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-pull": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db pull --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db push --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-status": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate status --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate reset --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-resolve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate resolve --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db seed --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    },
    "prisma-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma validate --schema libs/integrations/prisma-integrations/prisma/schema.prisma",
        "envFile": "apps/integrations/.env"
      }
    }
  },
  "tags": []
}
